            <property name="queryText">SELECT DISTINCT
       Organismo.ORG_NOMBRE,
       ExpNumero.NRO_NUMERO,
       ExpNumero.EXP_CARATULA,
       ExpNumero.ESTADO,
       CASE
           WHEN TO_CHAR(Expediente.EXP_FECHA_INICIO,'DD/MM/YYYY') = '01/01/1600' THEN
               TO_DATE(NULL,'DD/MM/YYYY')
           ELSE
               Expediente.EXP_FECHA_INICIO
       END AS EXP_FECHA_INICIO,
       Expediente.EXP_ID,
       Expediente.ORG_ID,
       Expediente.EXP_CUIJ,
       Expediente.EXP_SUFIJO,
       ExpNumero.NRO_ANIO,
       ExpNumero.NRO_BIS,
       '' AS EXPEDIENTE,
       to_char(EXPEDIENTE.FECHA_ACTUALIZACION,'dd/MM/yyyy') as FECHA_ACTUALIZACION,
       to_char(EXP_FECHA_INICIO,'dd/MM/yyyy') as EXP_FECHA_INICIO_S,
       loc.loc_descr
FROM Expediente
INNER JOIN Organismo ON (Organismo.ORG_ID = Expediente.ORG_ID AND Organismo.ESTADO = 0 AND Organismo.ORG_VISIBLE = 1)
INNER JOIN EXP_NUMERO ExpNumero ON (Expediente.EXP_ID = ExpNumero.EXP_ID AND Organismo.ORG_ID = ExpNumero.ORG_ID)
LEFT JOIN LOCALIDAD loc ON (Expediente.LOC_ID = loc.LOC_ID and loc.ESTADO = 0 and loc.LOC_VISIBLE = 1 )
LEFT JOIN COMPETENCIA com ON (Organismo.COM_ID = com.COM_ID AND com.ESTADO = 0 AND com.COM_VISIBLE = 1)
LEFT JOIN Representante ON (Representante.EXP_ID = Expediente.EXP_ID AND Representante.ESTADO = 0) -- req 2630
LEFT JOIN Actuacion ON (Actuacion.EXP_ID = Expediente.EXP_ID AND Actuacion.ESTADO = 0) -- req 1012
INNER JOIN VISIBILIDAD_ORG_IURIX_IOL vis ON (Expediente.ORG_ID = vis.ORG_ID)
WHERE Expediente.ESTADO = 0
AND Expediente.EXP_VISIBLE = 'S'
AND vis.ESTADO = 0</property>
