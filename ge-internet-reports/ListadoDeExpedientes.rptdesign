            <method name="beforeOpen"><![CDATA[var where = " ";


if (params["param_cuij"].value ) {
	where += " AND Expediente.EXP_CUIJ = '" +  params["param_cuij"].value+"'";
}

if (params["param_org_id"].value ) {
	where += " AND Organismo.ORG_ID = " +  params["param_org_id"].value;
}
if (params["param_loc_id"].value) {
	where += " AND ExpNumero.LOC_ID = " +  params["param_loc_id"].value;
}

if (params["param_id_profesional"].value ) {
    if (params["param_id_profesional"].value != -1)
	where += " and Representante.PRO_ID =  "+  params["param_id_profesional"].value  ;
}

if (params["param_nro_exp"].value ) {
	where += " and ExpNumero.NRO_NUMERO = "+  params["param_nro_exp"].value  ;
}

if (params["param_anio_exp"].value ) {
	where += " and ExpNumero.NRO_ANIO = "+  params["param_anio_exp"].value  ;
}

if (params["param_bis"].value ) {
	where += " and ExpNumero.NRO_BIS = '"+  params["param_bis"].value+"'"  ;
}

if (params["param_sufijo"].value ) {
	where += " and Expediente.EXP_SUFIJO = '"+  params["param_sufijo"].value+"'"  ;
}

if (params["param_excluirArchivados"].value ) {
	where += " AND (Expediente.EXP_UBICACION IS NULL OR NOT Expediente.EXP_UBICACION LIKE '"+params["param_excluirArchivados"].value+"%')";
}

if (params["param_diasNovedades"].value) {
	where += " AND TRUNC(Expediente.FECHA_ACTUALIZACION) >= current_date -"+params["param_diasNovedades"].value;
}

if (params["param_fecNotFirmaDigDesde"].value && params["param_fecNotFirmaDigHasta"].value) {
    where += " AND TRUNC(ACTUACION.ACT_FECFIR) BETWEEN '"+params["param_fecNotFirmaDigDesde"].value
    		+"' AND '"+params["param_fecNotFirmaDigHasta"].value
    		+ "' AND ACTUACION.ACT_TIPO=3 AND ACTUACION.ACT_TIPO_FIRMA=1";
} else if (params["param_fecNotFirmaDigDesde"].value && params["param_fecNotFirmaDigHasta"].value == '') {
	where += " AND TRUNC(ACTUACION.ACT_FECFIR) >= '"+params["param_fecNotFirmaDigDesde"].value
    		+ "' AND ACTUACION.ACT_TIPO=3 AND ACTUACION.ACT_TIPO_FIRMA=1";
} else if (params["param_fecNotFirmaDigDesde"].value == '' && params["param_fecNotFirmaDigHasta"].value) {
	where += " AND TRUNC(ACTUACION.ACT_FECFIR) <= '"+params["param_fecNotFirmaDigHasta"].value
    		+ "' AND ACTUACION.ACT_TIPO=3 AND ACTUACION.ACT_TIPO_FIRMA=1";
}

where += " ORDER BY Expediente.EXP_CUIJ ASC"
if (where != ""){
	this.queryText += where;
}
Packages.java.lang.System.out.println("queryText: " + queryText);]]></method>
